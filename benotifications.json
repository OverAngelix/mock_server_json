        "type": "POST",
        "url": "/sendFCM/{fcmToken}",
        "requestModel": {
          "title": "string",
          "body": "string",
          "isAlreadyRead": "boolean",
          "isDeleted": "boolean",
          "date": "datetime",
          "type": "string",
