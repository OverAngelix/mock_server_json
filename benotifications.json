    {
      "name": "get-history",
      "information": "Retourne l'historique des notifications, en excluant celles marquées comme supprimées.",
      "methods": {
        "type": "GET",
        "url": "/history",
        "Headers": {
          "Authorization": "Bearer OAUTHTOKEN"
        },
        "responseModel": [
          {
            "id": 0,
            "title": "string",
            "body": "string",
            "isAlreadyRead": "boolean",
            "isDeleted": "boolean",
            "date": "datetime",
            "type": "string",
            "urlExterne": "string",
            "urlActualite": "string"
          }
        ]
      }
    },
    {
      "name": "change-history",
      "information": "Envoie un tableau de notification et modifie les etat lues et/ou supprimées des notifications du tableau",
      "methods": {
        "type": "POST",
        "url": "/history",
        "Headers": {
          "Authorization": "Bearer OAUTHTOKEN"
        },
        "requestModel": [
          {
            "id": 0,
            "isAlreadyRead": "boolean",
            "isDeleted": "boolean"
          }
        ],
        "responseModel": "200 OK"
      }
    },
