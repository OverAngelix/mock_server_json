    {
      "name": "subscribe-to-topic",
      "information": "Abonne un appareil à un topic Firebase Cloud Messaging.",
      "methods": [
        {
          "type": "PUT",
          "url": "/terminal/{fcmoken}",
          "Headers": {
            "Authorization": "Bearer OAUTHTOKEN"
          },
          "responseModel": "200 OK"
        }
      ]
    },
    {
      "name": "unsubscribe-from-topic",
      "information": "Désabonne un appareil d'un topic Firebase Cloud Messaging.",
      "methods": [
        {
          "type": "DELETE",
          "url": "/terminal/{fcmoken}",
          "Headers": {
            "Authorization": "Bearer OAUTHTOKEN"
          },
          "responseModel": "200 OK"
        }
      ]
    },
    {
      "name": "send-topic",
      "information": "Envoie une notification  à un appareil spécifique via un token d'enregistrement FCM (firebase)",
